@using BoxOffice.Models
@model BoxOffice.Models.Movie
@{
    ViewBag.Title = Model.Name;
}
@section subMenu
{
    @Html.Partial(partialViewName: "SubMenuPartial")
}

@section js
{
    <script type="text/javascript" src="@Url.Content("~/Scripts/jquery.raty.min.js")"></script>
    <script type="text/javascript">
        var page = "#movies";
        var subpage = "#movieDetailsLink";
        $(document).ready(function() {
            $(page).addClass("selected");
            $(subpage).addClass("selected");
        });
        
        // rating
        $(".rating").raty({
            half: true,
            halfShow: true,
            number: 10,
            path: "/Content/img",
            readOnly: true,
            start: function () {
                $(this).attr("data-rating");
            }
        });
    </script>
}

<section id="movieArea" class="containerMovie">
    @if (Model.Images.Count() != 0)
    {
        <img class="item" src="@Model.Images.First().Url" alt=@string.Format("{0} movie poster", Model.Name) />
    }
    else
    {
        <img alt=@string.Format("No poster for {0} available", Model.Name) class="item" src="data:image/jpg;base64," />
    }
    <section id="infoArea" class="item">
        <h2>@Model.Name</h2>
        <fieldset>
            <legend>Community Ratings</legend>
            @if (Model.Ratings.Any())
            {
                <label>Rating:</label>
                <div class="rating" data-rating="@Model.Rating"></div>
            }
            else
            {
                <label>Rating (TMDb):</label>
                <div class="rating" data-rating="@Model.Rating_by_moviedb"></div>
            }
            @if (Model.Ratings.Any())
            {
                <label class="rightish">Votes:</label>
                <span>@Model.Votes</span>
            }
            else
            {
                <label class="rightish">Votes (TMDb):</label>
                <span>@Model.Votes_by_moviedb</span>
            }
        </fieldset>
        <section id="story">
            @if (!string.Empty.Equals(value: Model.Tagline))
            {
                <div id="tagline">
                    <label>Tagline</label>
                    <p>@Model.Tagline</p>
                </div>
            }
            <div id="overview">
                <label>Overview</label>
                <p>@Model.Overview</p>
            </div>
        </section>
        @* Show buttons only for logged in users *@
        @if (Request.IsAuthenticated)
        {
            <section id="actions" class="ui-dialog-buttonpane ui-widget-content ui-helper-clearfix">
                <div class="ui-dialog-buttonset">
                    <button type="button" class="ui-button ui-widget ui-state-default ui-corner-all ui-button-text-only"
                        role="button" aria-disabled="false">
                        @if (Model.isRentable)
                        {
                            <a href="javascript:addToQueue(@Model.MovieID);"><span class="ui-button-text" id="addToQueue">
                                Add to Queue </span></a>
                        }
                        else
                        {
                            <a href="#"><span class="ui-button-text ui-state-disabled" id="addToQueue">Add to Queue
                            </span></a>
                        }
                    </button>
                    @{
                        // compute, whether the user already watched the movie
                        var db = new BoxOfficeContext();
                        var theUser = (from u in db.Users
                                       where u.Username == User.Identity.Name
                                       select u).First();

                        // check if movie is in rental list
                        bool isWatched = false;
                        var rentalFromQueue = new Rental();
                        foreach (var rental in theUser.Queue)
                        {
                            // if in queue, check if watched
                            if ((rental.MovieID == Model.MovieID) && (rental.DateSent != null))
                            {
                                // if a DVD was sent to the user, we assume it was watched
                                isWatched = true;
                                break;
                            }
                        }
                    }
                    <button type="button" class="ui-button ui-widget ui-state-default ui-corner-all ui-button-text-only"
                        role="button" aria-disabled="false">
                        @if (isWatched)
                        {
                            <a href="javascript:rate(@Model.MovieID);"><span class="ui-button-text" id="rate">Rate
                            </span></a>
                        }
                        else
                        {
                            <a href="#"><span class="ui-button-text ui-state-disabled" id="rate">Rate </span>
                            </a>
                        }
                    </button>
                    <button type="button" class="ui-button ui-widget ui-state-default ui-corner-all ui-button-text-only"
                            role="button" aria-disabled="false">
                        @if (isWatched)
                        {
                            <a href="javascript:Comment(@Model.MovieID);"><span class="ui-button-text" id="comment">
                                                                          Comment </span></a>
                        }
                        else
                        {
                            <a href="#"><span class="ui-button-text ui-state-disabled" id="comment">
                                                                          Comment </span></a>
                        }
                        
                    </button>
                </div>
            </section>
        }
        <section id="comments">
            <label>Comments</label>
            @foreach (var comment in Model.Comments.ToList())
            {
                <section class="comment">
                    <label>@comment.User.Username (@comment.Date)</label>
                    <a href="javascript:flagComment(@comment.CommentID)">Flag comment</a>
                    <p>@comment.Message</p>
                </section>
            }
        </section>
    </section>
</section>
